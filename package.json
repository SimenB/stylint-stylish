{
  "name": "stylint-stylish",
  "version": "1.2.0",
  "description": "A stylish reporter for Stylint, heavily inspired by jshint-stylish",
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "scripts": {
    "clean": "rimraf coverage/ index.js test-es5.js",
    "compile": "babel stylish.js -o index.js",
    "compile-test": "babel test.js -o test-es5.js",
    "cover": "babel-istanbul cover -x test-es5.js _mocha test-es5",
    "lint": "standard",
    "precover": "npm run lint && npm run compile-test",
    "prelint": "npm run clean",
    "prepublish": "npm run compile",
    "pretest": "npm run lint",
    "test": "mocha --compilers js:babel/register"
  },
  "repository": "SimenB/stylint-stylish",
  "keywords": [
    "lint",
    "reporter",
    "stylint",
    "stylint-reporter",
    "stylish",
    "stylus"
  ],
  "author": "Simen Bekkhus <sbekkhus91@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SimenB/stylint-stylish/issues"
  },
  "homepage": "https://github.com/SimenB/stylint-stylish#readme",
  "dependencies": {
    "chalk": "^1.0.0",
    "lodash.isnumber": "^3.0.0",
    "log-symbols": "^1.0.0",
    "path-is-absolute": "^1.0.0",
    "pluralize": "^1.0.0",
    "text-table": "^0.2.0"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-istanbul": "^0.3.20",
    "codeclimate-test-reporter": "^0.1.1",
    "coveralls": "^2.11.4",
    "lodash.clonedeep": "^3.0.2",
    "mocha": "^2.3.2",
    "rimraf": "^2.4.3",
    "standard": "^5.3.1",
    "stylint": "^1.2.2"
  },
  "peerDependencies": {
    "stylint": ">= 1.0.0"
  }
}
